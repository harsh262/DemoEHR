patientSearchApp.service('patientSearchService',['$http','urlConfig',function($http,urlConfig) {
	// service to get all patients
	this.fetchAllPatientList = function(){
		var URI = urlConfig.url + 'doctors/'+getCookie("doctorId")+'/patients';
		var promis = $http.get(URI)
		.then(function(response){
			console.log(response.data.result.patients);
			if(response.data._status === 'done'){
				console.log("success");
				return (response.data.result.patients);
			}
		},
		function() {
			alert("error");
		})
		return promis;
	};
	
